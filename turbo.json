{
    "$schema": "https://turbo.build/schema.json",
    "tasks": {
        "build": {
            "dependsOn": [
                "^build"
            ],
            "outputs": []
        },
        "test": {
            "dependsOn": [
                "build"
            ],
            "outputs": [
                "coverage/**"
            ]
        },
        "lint": {
            "outputs": []
        },
        "dev": {
            "cache": false
        },
        "preview": {
            "cache": false
        }
    },
    "globalDependencies": [
        "pnpm-lock.yaml",
        ".env"
    ]
}